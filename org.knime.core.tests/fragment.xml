<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.knime.testing.TestcaseCollector">
      <TestcaseCollector
            TestcaseCollector="org.knime.core.CoreTestcaseCollector">
      </TestcaseCollector>
   </extension>
   <extension
         point="org.knime.core.DataValueRenderer">
      <dataValue
            valueClass="org.knime.core.data.renderer.TestDataValue">
         <renderer
               rendererFactoryClass="org.knime.core.data.renderer.StringValueRenderer$Factory"
               suggestAsDefault="false">
         </renderer>
         <renderer
               rendererFactoryClass="org.knime.core.data.renderer.TestValueRenderer$Factory"
               suggestAsDefault="true">
         </renderer>
      </dataValue>
   </extension>
   <extension
         point="org.knime.core.EarlyStartup">
      <provider
            class="org.knime.core.util.EarlyStartupTest">
      </provider>
   </extension>
   <extension
         point="org.knime.core.WorkflowSaveHook">
      <WorkflowSaveHook
            class="org.knime.core.util.TestWorkflowSaveHook">
      </WorkflowSaveHook>
   </extension>
</fragment>
